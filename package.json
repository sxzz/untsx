{
  "name": "untsx",
  "version": "0.1.1",
  "packageManager": "pnpm@10.13.1",
  "description": "Universal TypeScript extension for all popular toolchains.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/untsx#readme",
  "bugs": {
    "url": "https://github.com/sxzz/untsx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/untsx.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@typescript-eslint/typescript-estree": "^8.38.0",
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
    "acorn-typescript": "^1.4.13",
    "espree": "^10.4.0",
    "estree-walker": "^3.0.3",
    "magic-string-ast": "^1.0.0",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@sxzz/eslint-config": "^7.0.6",
    "@sxzz/prettier-config": "^2.2.3",
    "@sxzz/test-utils": "^0.5.8",
    "@ts-macro/language-plugin": "^0.3.1",
    "@types/espree": "^10.1.0",
    "@types/node": "^24.0.15",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "tc39-try": "^0.3.2",
    "ts-macro": "^0.3.1",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
